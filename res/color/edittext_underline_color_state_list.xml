<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:twitter="http://schemas.android.com/apk/res-auto">
    <item
        android:state_enabled="false"
        android:color="?coreColorSectionDivider" />
    <item
        android:color="@color/destructive_red"
        twitter:state_fault="true" />
    <item
        android:color="@color/destructive_red"
        twitter:state_error="true" />
    <item
        android:color="@color/yellow_500"
        twitter:state_warning="true" />
    <item
        android:state_focused="true"
        android:color="@color/twitter_blue" />
    <item
        android:state_pressed="true"
        android:color="@color/twitter_blue" />
    <item
        android:color="?coreColorTertiary" />
</selector>