<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:twitter="http://schemas.android.com/apk/res-auto">
    <item
        android:drawable="@drawable/ic_sign_up_reveal_invalid_on"
        twitter:state_error="true"
        twitter:state_password_reveal="true" />
    <item
        android:drawable="@drawable/ic_sign_up_reveal_invalid_default"
        twitter:state_error="true" />
    <item
        android:drawable="@drawable/ic_sign_up_reveal_valid_on"
        twitter:state_password_reveal="true"
        twitter:state_validated="true" />
    <item
        android:drawable="@drawable/ic_sign_up_reveal_valid_default"
        twitter:state_validated="true" />
    <item
        android:drawable="@drawable/ic_sign_up_reveal_only_on"
        twitter:state_password_reveal="true" />
    <item
        android:drawable="@drawable/ic_sign_up_reveal_only_default" />
</selector>