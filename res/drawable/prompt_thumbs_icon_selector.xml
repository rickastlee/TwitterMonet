<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_focused="false"
        android:state_pressed="false">
        <shape
            android:shape="oval"
            xmlns:android="http://schemas.android.com/apk/res/android">
            <stroke
                android:width="1.0dip"
                android:color="?abstractColorLightGray" />
        </shape>
    </item>
    <item
        android:state_pressed="true">
        <shape
            android:shape="oval"
            xmlns:android="http://schemas.android.com/apk/res/android">
            <stroke
                android:width="1.0dip"
                android:color="?abstractColorLightGray" />
            <solid
                android:color="?abstractColorMediumGray" />
        </shape>
    </item>
    <item
        android:state_focused="true">
        <shape
            android:shape="oval"
            xmlns:android="http://schemas.android.com/apk/res/android">
            <stroke
                android:width="1.0dip"
                android:color="?abstractColorLightGray" />
            <solid
                android:color="?abstractColorMediumGray" />
        </shape>
    </item>
</selector>