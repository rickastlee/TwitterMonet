<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:twitter="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@id/live_event_card_score_card_layout"
        android:visibility="gone"
        android:layout_width="@dimen/live_event_card_score_card_width"
        android:layout_height="@dimen/live_event_card_score_card_height"
        android:importantForAccessibility="yes">
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@id/live_event_card_score_card_caret"
            android:layout_height="wrap_content"
            android:textAlignment="viewEnd"
            twitter:layout_constraintBottom_toBottomOf="@id/live_event_card_score_card_status"
            twitter:layout_constraintEnd_toEndOf="parent"
            twitter:layout_constraintTop_toTopOf="@id/live_event_card_score_card_status"
            style="@style/LiveEventCardCaret" />
        <com.twitter.media.ui.fresco.FrescoMediaImageView
            android:id="@id/live_event_card_score_card_top_team_avatar"
            android:background="@drawable/live_event_score_card_top_start_corner_bg"
            android:padding="@dimen/live_event_card_score_card_avatar_image_padding"
            android:layout_width="@dimen/live_event_card_score_card_avatar_wrapper_size"
            android:layout_height="@dimen/live_event_card_score_card_avatar_wrapper_size"
            android:contentDescription="@null"
            android:importantForAccessibility="no"
            twitter:defaultDrawable="@drawable/ic_white_circle_opacity_40"
            twitter:layout_constraintStart_toStartOf="parent"
            twitter:layout_constraintTop_toTopOf="parent"
            twitter:placeholderImage="@drawable/ic_white_circle_opacity_40"
            twitter:roundingStrategy="circle"
            twitter:scaleMode="fit"
            style="@style/LiveEventScoreCardTopTeam" />
        <com.twitter.media.ui.fresco.FrescoMediaImageView
            android:id="@id/live_event_card_score_card_bottom_team_avatar"
            android:background="@drawable/live_event_score_card_bottom_start_corner_bg"
            android:padding="@dimen/live_event_card_score_card_avatar_image_padding"
            android:layout_width="@dimen/live_event_card_score_card_avatar_wrapper_size"
            android:layout_height="@dimen/live_event_card_score_card_avatar_wrapper_size"
            android:layout_marginBottom="@dimen/live_event_card_score_card_margin_size"
            android:contentDescription="@null"
            android:importantForAccessibility="no"
            twitter:defaultDrawable="@drawable/ic_white_circle_opacity_40"
            twitter:layout_constraintStart_toStartOf="parent"
            twitter:layout_constraintTop_toBottomOf="@id/live_event_card_score_card_top_team_avatar"
            twitter:placeholderImage="@drawable/ic_white_circle_opacity_40"
            twitter:roundingStrategy="circle"
            twitter:scaleMode="fit"
            style="@style/LiveEventScoreCardBottomTeam" />
        <com.twitter.ui.components.text.legacy.TypefacesTextView
            android:gravity="start|center"
            android:id="@id/live_event_card_score_card_top_team_name"
            android:layout_width="@dimen/live_event_card_score_card_match_constraint"
            android:layout_height="@dimen/live_event_card_score_card_match_constraint"
            android:importantForAccessibility="no"
            twitter:layout_constraintBottom_toBottomOf="@id/live_event_card_score_card_top_team_avatar"
            twitter:layout_constraintEnd_toStartOf="@id/live_event_card_score_card_top_team_score"
            twitter:layout_constraintStart_toEndOf="@id/live_event_card_score_card_top_team_avatar"
            twitter:layout_constraintTop_toTopOf="@id/live_event_card_score_card_top_team_avatar"
            style="@style/LiveEventScoreCardTopTeamNameText" />
        <com.twitter.ui.components.text.legacy.TypefacesTextView
            android:gravity="end|center"
            android:id="@id/live_event_card_score_card_top_team_score"
            android:paddingTop="@dimen/space_2"
            android:paddingBottom="@dimen/space_4"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/live_event_card_score_card_match_constraint"
            android:includeFontPadding="false"
            android:importantForAccessibility="no"
            android:paddingStart="@dimen/live_event_card_score_card_item_gap"
            android:paddingEnd="@dimen/live_event_card_score_card_item_gap"
            twitter:layout_constraintBottom_toBottomOf="@id/live_event_card_score_card_top_team_avatar"
            twitter:layout_constraintEnd_toEndOf="parent"
            twitter:layout_constraintStart_toEndOf="@id/live_event_card_score_card_top_team_name"
            twitter:layout_constraintTop_toTopOf="@id/live_event_card_score_card_top_team_avatar"
            style="@style/LiveEventScoreCardTopTeamScoreText" />
        <com.twitter.ui.components.text.legacy.TypefacesTextView
            android:gravity="start|center"
            android:id="@id/live_event_card_score_card_bottom_team_name"
            android:layout_width="@dimen/live_event_card_score_card_match_constraint"
            android:layout_height="@dimen/live_event_card_score_card_match_constraint"
            android:importantForAccessibility="no"
            twitter:layout_constraintBottom_toBottomOf="@id/live_event_card_score_card_bottom_team_avatar"
            twitter:layout_constraintEnd_toStartOf="@id/live_event_card_score_card_bottom_team_score"
            twitter:layout_constraintStart_toEndOf="@id/live_event_card_score_card_bottom_team_avatar"
            twitter:layout_constraintTop_toTopOf="@id/live_event_card_score_card_bottom_team_avatar"
            style="@style/LiveEventScoreCardBottomTeamNameText" />
        <com.twitter.ui.components.text.legacy.TypefacesTextView
            android:gravity="end|center"
            android:id="@id/live_event_card_score_card_bottom_team_score"
            android:paddingTop="@dimen/space_2"
            android:paddingBottom="@dimen/space_4"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/live_event_card_score_card_match_constraint"
            android:includeFontPadding="false"
            android:importantForAccessibility="no"
            android:paddingStart="@dimen/live_event_card_score_card_item_gap"
            android:paddingEnd="@dimen/live_event_card_score_card_item_gap"
            twitter:layout_constraintBottom_toBottomOf="@id/live_event_card_score_card_bottom_team_avatar"
            twitter:layout_constraintEnd_toEndOf="parent"
            twitter:layout_constraintStart_toEndOf="@id/live_event_card_score_card_bottom_team_name"
            twitter:layout_constraintTop_toTopOf="@id/live_event_card_score_card_bottom_team_avatar"
            style="@style/LiveEventScoreCardBottomTeamScoreText" />
        <com.twitter.ui.components.text.legacy.TypefacesTextView
            android:textStyle="bold"
            android:gravity="start|center"
            android:id="@id/live_event_card_score_card_status"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/space_8"
            android:includeFontPadding="false"
            android:importantForAccessibility="no"
            android:paddingEnd="@dimen/space_2"
            twitter:layout_constrainedWidth="true"
            twitter:layout_constraintEnd_toStartOf="@id/live_event_card_score_card_divider"
            twitter:layout_constraintHorizontal_bias="0.0"
            twitter:layout_constraintHorizontal_chainStyle="packed"
            twitter:layout_constraintStart_toStartOf="parent"
            twitter:layout_constraintTop_toBottomOf="@id/live_event_card_score_card_bottom_team_avatar"
            twitter:layout_constraintWidth_max="wrap"
            twitter:layout_constraintWidth_min="@dimen/live_event_card_score_card_constraint_min"
            style="@style/LiveEventScoreCardStatusText" />
        <com.twitter.ui.components.text.legacy.TypefacesTextView
            android:gravity="center"
            android:id="@id/live_event_card_score_card_divider"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/live_events_card_score_card_game_info_end_in_progress"
            android:includeFontPadding="false"
            android:importantForAccessibility="no"
            android:paddingStart="@dimen/space_2"
            android:paddingEnd="@dimen/space_2"
            twitter:layout_constrainedWidth="true"
            twitter:layout_constraintBaseline_toBaselineOf="@id/live_event_card_score_card_status"
            twitter:layout_constraintEnd_toStartOf="@id/live_event_card_score_card_info"
            twitter:layout_constraintStart_toEndOf="@id/live_event_card_score_card_status"
            twitter:layout_constraintTop_toTopOf="@id/live_event_card_score_card_status"
            twitter:layout_constraintWidth_max="wrap"
            twitter:layout_constraintWidth_min="1.0dip"
            style="@style/LiveEventScoreCardStatusText" />
        <com.twitter.ui.components.text.legacy.TypefacesTextView
            android:gravity="start|center"
            android:id="@id/live_event_card_score_card_info"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:includeFontPadding="false"
            android:importantForAccessibility="no"
            android:paddingStart="@dimen/space_2"
            android:paddingEnd="@dimen/space_4"
            twitter:layout_constrainedWidth="true"
            twitter:layout_constraintBaseline_toBaselineOf="@id/live_event_card_score_card_divider"
            twitter:layout_constraintEnd_toStartOf="@id/live_event_card_score_card_caret"
            twitter:layout_constraintStart_toEndOf="@id/live_event_card_score_card_divider"
            twitter:layout_constraintTop_toTopOf="@id/live_event_card_score_card_divider"
            twitter:layout_constraintWidth_max="wrap"
            twitter:layout_constraintWidth_min="@dimen/live_event_card_score_card_constraint_min"
            style="@style/LiveEventScoreCardStatusText" />
        <com.twitter.ui.components.text.legacy.TypefacesTextView
            android:gravity="start|center"
            android:id="@id/live_event_card_score_card_category"
            android:layout_width="@dimen/live_event_card_score_card_match_constraint"
            android:layout_height="wrap_content"
            android:includeFontPadding="false"
            android:importantForAccessibility="no"
            android:textAlignment="viewStart"
            android:paddingEnd="@dimen/live_event_card_score_card_item_gap"
            twitter:layout_constraintEnd_toStartOf="@id/live_event_card_score_card_caret"
            twitter:layout_constraintStart_toStartOf="parent"
            twitter:layout_constraintTop_toBottomOf="@id/live_event_card_score_card_status"
            twitter:layout_goneMarginTop="@dimen/font_size_xxsmall"
            style="@style/LiveEventScoreCardCategoryText" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</merge>