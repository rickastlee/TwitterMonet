        android:layout_marginEnd="@dimen/tweet_avatar_start_margin"
        app:layout_constraintEnd_toStartOf="@id/tweet_header"
        app:layout_constraintTag="social_context_badge"
        app:layout_constraintTop_toTopOf="@id/tweet_social_context">
