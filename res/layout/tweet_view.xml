        android:layout_marginTop="@dimen/social_context_tweet_header_padding_topic_context"
        android:layout_marginStart="?timelineRowHorizontalPadding"
        android:layout_marginEnd="@dimen/image_margin_end"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTag="tweet_profile_image_container"
        app:layout_constraintTop_toBottomOf="@id/tweet_social_context"
        app:layout_goneMarginTop="@dimen/social_context_tweet_header_padding_general_context">
