        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/space_4"
        app:layout_constraintBottom_toBottomOf="@id/tweet_curation_action"
        app:layout_constraintEnd_toStartOf="@id/tweet_curation_action"
        app:layout_constraintStart_toEndOf="@id/tweet_header"
        app:layout_constraintTop_toTopOf="@id/tweet_curation_action">
