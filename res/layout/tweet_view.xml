            android:value="SocialContextBadge"
            android:id="@id/weaverComponent" />
    </com.twitter.ui.widget.TintableImageView>
    <include
        android:id="@id/tweet_social_context"
        android:layout_width="0.0dip"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/tweet_profile_image_container"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/social_context_badge_barrier_margin"
        app:layout_constraintTop_toBottomOf="@id/social_context_education"
        layout="@layout/component_social_context" />
