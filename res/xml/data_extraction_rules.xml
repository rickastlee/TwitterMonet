<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup
        disableIfNoEncryptionCapabilities="true">
        <exclude
            domain="file" />
        <exclude
            domain="database" />
        <exclude
            domain="sharedpref" />
        <exclude
            domain="external" />
        <exclude
            domain="root"
            path="code_cache" />
        <exclude
            domain="root"
            path="lib-main" />
    </cloud-backup>
    <device-transfer>
        <exclude
            domain="root"
            path="lib-main" />
    </device-transfer>
</data-extraction-rules>