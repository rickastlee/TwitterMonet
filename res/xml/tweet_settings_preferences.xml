<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:twitter="http://schemas.android.com/apk/res-auto">
    <com.twitter.ui.widget.PreferenceTopCategoryCompat
        twitter:allowDividerAbove="false"
        twitter:allowDividerBelow="false"
        twitter:key="tweet_category"
        twitter:title="@string/loading" />
    <com.twitter.settings.widget.LinkablePreferenceCompat
        android:selectable="false"
        android:summary="@string/settings_notif_tweets_live_follow_summary"
        twitter:alwaysDisplayUrl="true"
        twitter:clickableUrl="@string/tweet_settings_help_text_learn_more" />
</PreferenceScreen>